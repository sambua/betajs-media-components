his.\n","maintainers":[{"name":"stevefar","email":"me@stevefar.com"}],"time":{"modified":"2022-06-19T01:44:00.136Z","created":"2014-01-28T17:42:06.262Z","0.0.1":"2014-01-28T17:42:06.900Z","0.0.2":"2014-01-28T18:45:41.133Z","0.0.3":"2015-02-17T18:19:12.127Z","0.1.1":"2015-05-19T13:15:28.251Z","0.1.2":"2015-07-29T12:35:56.903Z"},"readmeFilename":"README.md","author":{"name":"Steven Farthing"},"repository":{"type":"git","url":"https://github.com/sfarthin/intercept-stdout"},"homepage":"https://github.com/sfarthin/intercept-stdout","keywords":["stdout","stderr"],"bugs":{"url":"https://github.com/sfarthin/intercept-stdout/issues"},"license":"MIT","users":{"chriszs":true}}